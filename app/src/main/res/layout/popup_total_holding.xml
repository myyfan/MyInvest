<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_totalHolding_LinearLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#fff"
    android:orientation="vertical"
    android:layout_centerInParent="true">
    <Button
        android:id="@+id/layout_changeGained_button_copyBoard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="复制并打开微信"
        android:layout_margin="@dimen/layout_addstock_margin"/>
    <TextView
        android:id="@+id/layout_totalHolding_editText_output"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/layout_addstock_margin">
    </TextView>
</LinearLayout>
